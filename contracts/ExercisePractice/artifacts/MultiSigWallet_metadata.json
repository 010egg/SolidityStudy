		"devdoc": {
			"details": "A multi-signature wallet contract that requires multiple confirmations before executing transactions.",
			"kind": "dev",
			"methods": {
				"cancelTransaction(uint256)": {
					"details": "Allows an owner to cancel a transaction before it is executed.",
					"params": {
						"_txIndex": "Transaction index"
					}
				},
				"confirmTransaction(uint256)": {
					"details": "Allows an owner to confirm a transaction.",
					"params": {
						"_txIndex": "Transaction index"
					}
				},
				"constructor": {
					"details": "Initializes the contract with a list of owners and the number of required confirmations.",
					"params": {
						"_owners": "List of initial owners",
						"_required": "Number of required confirmations"
					}
				},
				"executeTransaction(uint256)": {
					"details": "Executes a confirmed transaction.",
					"params": {
						"_txIndex": "Transaction index"
					}
				},
				"getOwners()": {
					"details": "Returns the list of owners.",
					"returns": {
						"_0": "Array of owner addresses"
					}
				},
				"getRequired()": {
					"details": "Returns the number of required confirmations.",
					"returns": {
						"_0": "Number of required confirmations"
					}
				},
				"getTransaction(uint256)": {
					"details": "Returns details of a specific transaction.",
					"params": {
						"_txIndex": "Transaction index"
					},
					"returns": {
						"canceled": "Whether the transaction has been canceled",
						"confirmationCount": "Number of confirmations",
						"data": "Transaction data",
						"executed": "Whether the transaction has been executed",
						"to": "Recipient address",
						"value": "Amount of Ether"
					}
				},
				"getTransactionCount()": {
					"details": "Returns the number of transactions.",
					"returns": {
						"_0": "Number of transactions"
					}
				},
				"isConfirmed(uint256,address)": {
					"details": "Returns whether an owner has confirmed a transaction.",
					"params": {
						"_owner": "Owner address",
						"_txIndex": "Transaction index"
					},
					"returns": {
						"_0": "Confirmation status"
					}
				},
				"revokeConfirmation(uint256)": {
					"details": "Allows an owner to revoke a confirmation for a transaction.",
					"params": {
						"_txIndex": "Transaction index"
					}
				},
				"submitTransaction(address,uint256,bytes)": {
					"details": "Allows an owner to submit a transaction.",
					"params": {
						"_data": "Transaction data",
						"_to": "Recipient address",
						"_value": "Amount of Ether to send"
					},
					"returns": {
						"txIndex": "The index of the submitted transaction"
					}
				}
